generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model order {
  id         Int      @id @default(autoincrement())
  price      String
  phone      String
  address    String
  status     String
  status_b    String
  is_deleted Boolean  @default(false)
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
  created_by String
  updated_by String
}

